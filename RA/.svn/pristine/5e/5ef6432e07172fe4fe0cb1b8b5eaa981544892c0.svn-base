              <div class="col-sm-7"><label><span class="fa fa-building"></span>&nbsp;&nbsp;Company Name :</label></div>
              <div class="col-sm-5">{{ post.companyName}}</div>
              </div>
              </div>
             <div class="col-sm-6">
             <div class="row">
          		 <div class="col-sm-6"><label><span class="fa fa-black-tie"></span>&nbsp;&nbsp;Job Type :</label></div>
              <div class="col-sm-6">{{post.jobTitle}}</div>
              </div>
              </div>
          
          </div>
        	
        	<div class="row l4">
          	<div class="col-sm-6">
          		<div class="row">
              <div class="col-sm-7"><label><span class="fa fa-briefcase"></span>&nbsp;&nbsp; Experience:</label></div>
              <div class="col-sm-5">{{post.experience}}</div>
              </div>
              </div>
             <div class="col-sm-6">
             <div class="row">
          		 <div class="col-sm-6"><label><span class="fa fa-map-marker"></span>&nbsp;&nbsp;Job Location :</label></div>
              <div class="col-sm-6">{{post.jobLocation}}</div>
              </div>
              </div>
          
          </div>
           <div class="row l4">
          	<div class="col-sm-6">
          		<div class="row">
              <div class="col-sm-7"><label><span class="fa fa-users"></span>&nbsp;&nbsp;No of Resources :</label></div>
              <div class="col-sm-5">{{post.resources}}</div>
              </div>
              </div>
             <div class="col-sm-6">
             <div class="row">
          		 <div class="col-sm-6"><label><span class="fa fa-calendar"></span>&nbsp;&nbsp;Join Within :</label></div>
              <div class="col-sm-6">{{post.joinWithin}}</div>
              </div>
              </div>
          
          </div>
          
          <div class="row l4">
          	<div class="col-sm-12">
          		<div class="row">
              <div class="col-sm-4"><label><span class="fa fa-wpforms"></span>&nbsp;&nbsp; Job Description :</label></div>
              <div class="col-sm-8">{{post. description}}</div>
              </div>
              </div>
             
             
          
          </div>
          <div class="row l4">
          	<div class="col-sm-12">


<div class="container-fluid m-t-50">
		<div class="row sea">
	  <div class="container">
		<h4 style="color: #337ab7">Search Your Resource :</h4>
		<div class="col-sm-3">
			<oi-select ng-model="search.primarySkills" multiple oi-options="v for v in skills"  placeholder="Skills Ex:html"required>
              </oi-select>	
		</div>
		<div class="col-sm-3">
			<input type="text" ifEmpty class="form-control" ng-model="search.jobCategory"
				placeholder="job Category">
		</div>
		<div class="col-sm-2">
			<input type="text" class="form-control" ng-model="search.city"
				placeholder="Location">
		</div>
		<div class="col-sm-2">
			<input type="text" class="form-control"
				ng-model="search.totalExperience" placeholder="Experience">
		</div>
		<div class="col-sm-2">
			<button type="button" class="btn btn-primary" ng-click="dynamicsearch(search.primarySkills,search.jobCategory,search.city,search.totalExperience)">
				<i class="fa fa-search"></i>&nbsp;Search
			</button>
		</div>
      </div>
     
	</div>
		
      </div>	
	<div class="container m-t-20">
		<div class="row">

					<div class="col-sm-3">	
					
						
						
	<div class="">
				<uib-accordion close-others="oneAtATime">
                <div uib-accordion-group class="panel-default"
					is-open="status.Skills">
					<uib-accordion-heading> Skills <i
						class="pull-right glyphicon"
						 ng-class="{'glyphicon glyphicon-minus': status.Skills, 'glyphicon glyphicon-plus': !status.open}" ng-click="sidelist()"></i>
					</uib-accordion-heading>
					<ul class="list-group" ng-repeat="(skil1,count) in skillslist.skillsMap">
					  <input type="checkbox" checklist-model="skil.skills" checklist-value="skil1" ng-click="checkbox()"> {{skil1}}({{count}}) &nbsp;


					</ul>
				</div>
					<div uib-accordion-group class="panel-default" is-open="status.Job ">
					<uib-accordion-heading> Job Category <i class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.Job , 'glyphicon glyphicon-plus': !status.open}" ng-click="sidelist()"></i>
					</uib-accordion-heading>
					<ul class=" list-group" ng-repeat="(jobcat,count) in skillslist.jobCategoryMap">
					 <input type="checkbox" checklist-model="job.jobCategory1" checklist-value="jobcat" ng-click="checkbox()"> {{jobcat}}({{count}}) &nbsp;
					</ul>
				</div>
				<div uib-accordion-group class="panel-default"
					is-open="status.Location">
					<uib-accordion-heading> Location<i
						class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.Location, 'glyphicon glyphicon-plus': !status.open}" ng-click="sidelist()"></i>
					</uib-accordion-heading>

					<ul class=" list-group" ng-repeat="(location1,count) in skillslist.locationMap">
					    <input type="checkbox" checklist-model="location2.city" checklist-value="location1" ng-click="checkbox()"> {{location1}}({{count}}) &nbsp;
					</ul>
				</div>			
				<div uib-accordion-group class="panel-default"
					is-open="status.Experience">
					<uib-accordion-heading> Experience <i
						class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.Experience, 'glyphicon glyphicon-plus': !status.open}" ng-click="sidelist()"></i>
					</uib-accordion-heading>

					<ul class=" list-group" ng-repeat="(exp,count) in skillslist.yearsOfExperienceMap">
					<input type="checkbox" checklist-model="exp2.totalExperience" checklist-value="exp" ng-click="checkbox()"> {{exp}}({{count}}) &nbsp;

				
					</ul>
				</div>
			 	<div uib-accordion-group class="panel-default"
					is-open="status.company">
					<uib-accordion-heading> Top Vendors <i
					
						class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.company, 'glyphicon glyphicon-plus': !status.open}" ng-click="sidelist()"></i>
					</uib-accordion-heading>
					<ul class=" list-group" ng-repeat="(vendor,count) in skillslist.vendorSet"">
						<input type="checkbox" checklist-model="customer1.customer" checklist-value="vendor" ng-click="checkbox()"> {{vendor}}({{count}}) &nbsp;
						
					</ul>
				</div>
				<div uib-accordion-group class="panel-default"
					is-open="status.company1">
					<uib-accordion-heading> Budget <i
						class="pull-right glyphicon"

						ng-class="{'glyphicon glyphicon-minus': status.company1, 'glyphicon glyphicon-plus': !status.open}" ng-click="sidelist()"></i>

					</uib-accordion-heading>
					<ul class=" list-group" ng-repeat="(budget,count) in skillslist.budgetMap"">
						<input type="checkbox" checklist-model="budget1.budget" checklist-value="budget" ng-click="checkbox()"> {{budget}}({{count}}) &nbsp;
						
					</ul>
				</div>
				<br>
			<!-- 	<center>
					<button type="submit" class="btn primary">Clear filters</button>
					<button type="submit" class="btn primary" ng-click="search1(search1)">Submit</button>
				</center> -->
			</div>
		</div>	

					<div class="col-sm-8">
						<div ng-repeat="x in resourcelist" class="box">
						
							
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="header-box">
								<p>
									<span title="Job Category">{{x.jobCategory}}</span><span title="Location">-{{x.currentLocation}}</span><span title="Experience">({{x.yearsOfExperience}}-{{x.monthsOfExperience}})</span> <span title="Resource ID"> {{x.resId}}</span>
								</p>
							</div></a>
								<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="row">
							<div class="companyName-box col-sm-7">
								<p><span title="Company Name">{{x.companyName}}</span></p>
							</div>
							<div class="col-sm-5">
							<span style="" class="iconcolor" >&#x20B9;</span>&nbsp;:<span style="" class="textcolor" title="Budget">{{x.budget}} /</span><span style="" class="textfont">{{x.rate}}</span> +<span style="" class="taxes"> {{x.gst}} </span></span>
							</div>
							</div></a>
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="description-box">
								<p title="Resource Name"><i class="fa fa-user-circle-o" aria-hidden="true"></i>&nbsp;&nbsp;{{x.firstName}}</p>
							</div></a>
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="row">
							<div class="description-box col-sm-7">
								<p>
									<i class="glyphicon glyphicon-envelope" > </i>&nbsp;&nbsp;<span title="Vendor EmailId">{{x.emailId}}&nbsp;</span>
								</p>
							</div>
							<div class="col-sm-5">
							<i
										class="fa fa-phone iconcolor " ></i>&nbsp;:<span class="textcolor" title="Vendor Contact Number">{{x.mobileNumber}}</span>
							</div>
							</div></a>
							
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="description-box">
								<p>
									<i class="glyphicon glyphicon-calendar"></i>&nbsp;{{x.availability}}
								</p>
							</div></a>
							
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="Skills-box">
								<p>
									<span class="iconcolor">Skills:</span>&nbsp;{{x.primarySkills.toLowerCase() }}{{x.secondarySkills.toLowerCase() }}<!-- <a class=" btn pull-right"
										ng-click="requirementsearch(x._id,x.primarySkills,x.jobCategory,x.currentLocation,x.yearsOfExperience)">
									Search requirement
									</a> -->
											
								</p>
								
							</div></a>
					<div class="Skills-box">
						<p>
								&nbsp;&nbsp;SoftLock&nbsp; :<a
								class="btn btn-sm btn-success butn"
								ng-click="softlockResource(x)">{{x.softLock}}</a>
							&nbsp;&nbsp;HardLock &nbsp;:<a class="btn btn-sm btn-danger butn"
								ng-click="hardlockResource(x)" disabled>{{x.hardLock}}</a>
								&nbsp;&nbsp;&nbsp;&nbsp; <!-- <a type="button" class="btn btn-sm btn-primary"  ng-click="filedownload(x._id)"   file-download="myBlobObject"  file-name="'DownloadResume.docx'" ><i class="fa fa-download"></i>&nbsp;Download Resume</a> -->
								<!-- <div class="col-md-6">
<li><img ng-src="data:image/jpeg;base64,{{user.file1_data}}"
style="width: 280px; height: 280px;" class="img-responsive"
ng-model="admin" /></li>
<li>{{user.file1}}</li>

</div> -->
								&nbsp;&nbsp;MSA Available&nbsp;:<a class="btn btn-sm btn-primary butn"
								ng-click="isSowUserResource  (x)">{{x.isSowUser}}</a>
						</p>
						
					</div>
							 
						</div>
				
				
			</div>
		</div>
		<div class="pull-right">  
     <!-- <pagination total-items="totalcount" ng-change="pageChanged()" items-per-page="pageSizeSelected" direction-links="true"
     ng-model="pageIndex" max-size="maxSize" class="pagination" boundary-links="true" rotate="false" num-pages="numPages"></pagination> -->
               <!--  <a class="btn btn-primary">Page: {{pageIndex}} / {{pageSizeSelected}}</a>   -->
                <ul uib-pagination total-items="totalCount" max-size="maxSize" items-per-page="pageSizeSelected" boundary-links="true" rotate="false" num-pages="numPages" direction-links="true" ng-model="pageIndex" ng-change="pageChanged()"></ul>
                </div>
	</div>
</div>
</div>

<div class="container-fluid m-t-50" autoscroll="true">
	<div class="row sea">
	  <div class="container">
		<h4 style="color: #337ab7">Search Your Resource :</h4>
		<div class="col-sm-3">
			<oi-select ng-model="search.skills" multiple oi-options="v for v in skills"  placeholder="Skills Ex:html"required>
              </oi-select>	
		</div>
		<div class="col-sm-3">
			<input type="text" class="form-control" ng-model="search.jobCategory"
				placeholder="job Category">
		</div>
		<div class="col-sm-2">
			<input type="text" class="form-control" ng-model="search.city"
				placeholder="Location">
		</div>
		<div class="col-sm-2">
			<input type="text" class="form-control"
				ng-model="search.totalExperience" placeholder="Experience">
		</div>
		<div class="col-sm-2">
			<button type="button" class="btn btn-primary">
				<i class="fa fa-search"></i>&nbsp;Search
			</button>
		</div>
      </div>
	</div>
	</div>
	<br>
	
	<div class="container">
		<div class="row">
			<div class="col-sm-3">		
						
			<div class="">
				<uib-accordion close-others="oneAtATime">
                <div uib-accordion-group class="panel-default"
					is-open="status.Skills">
					<uib-accordion-heading> Skills <i
						class="pull-right glyphicon"
						 ng-class="{'glyphicon glyphicon-minus': status.Skills, 'glyphicon glyphicon-plus': !status.open}" ng-click="allListsfunc()"></i>
					</uib-accordion-heading>
					<ul class="list-group" ng-repeat="(skill,count) in skillslist.skillsMap">
					   <input type="checkbox" checklist-model="skil.skills" checklist-value="skill" ng-click="checkbox()"> {{skill}}({{count}})
					</ul>
				</div>
					<div uib-accordion-group class="panel-default" is-open="status.Job ">
					<uib-accordion-heading> Job Category <i
						class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.Job , 'glyphicon glyphicon-plus': !status.open}" ng-click="allListsfunc()"></i>
					</uib-accordion-heading>
					<ul class=" list-group" ng-repeat="(jobcategory,count) in skillslist.jobCategoryMap">
					 <input type="checkbox" checklist-model="job.jobCategory1" checklist-value="jobcategory" ng-click="checkbox()" > {{jobcategory}}({{count}}) &nbsp;
					</ul>
				</div>
				<div uib-accordion-group class="panel-default"
					is-open="status.Location">
					<uib-accordion-heading> Location<i
						class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.Location, 'glyphicon glyphicon-plus': !status.open}" ng-click="allListsfunc()"></i>
					</uib-accordion-heading>

					<ul class=" list-group" ng-repeat="(location1,count) in skillslist.locationMap">
					    <input type="checkbox" checklist-model="location2.city" checklist-value="location1" ng-click="checkbox()"> {{location1}}({{count}}) &nbsp;
					</ul>
				</div>			
				<div uib-accordion-group class="panel-default"
					is-open="status.Experience">
					<uib-accordion-heading> Experience <i
						class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.Experience, 'glyphicon glyphicon-plus': !status.open}" ng-click="allListsfunc()"></i>
					</uib-accordion-heading>
					<ul class=" list-group" ng-repeat="(exp1,count) in skillslist.yearsOfExperienceMap">
					<input type="checkbox" checklist-model="exp2.totalExperience" checklist-value="exp1" ng-click="checkbox()"> {{exp1}}({{count}})&nbsp;
				
					</ul>
				</div>
			 	<div uib-accordion-group class="panel-default"
					is-open="status.company">
					<uib-accordion-heading> Top Vendors <i
						class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.company, 'glyphicon glyphicon-plus': !status.open}" ng-click="allListsfunc()"></i>
					</uib-accordion-heading>
					<ul class=" list-group" ng-repeat="(comp,count) in skillslist.vendorSet">
						<input type="checkbox" checklist-model="customer1.customer" checklist-value="comp" ng-click="checkbox()"> {{comp}}({{count}}) &nbsp;
						
					</ul>
				</div>
				<div uib-accordion-group class="panel-default"
					is-open="status.company1">
					<uib-accordion-heading> Budget <i
						class="pull-right glyphicon"
						ng-class="{'glyphicon glyphicon-minus': status.company1, 'glyphicon glyphicon-plus': !status.open}" ng-click="allListsfunc()"></i>
					</uib-accordion-heading>
					<ul class=" list-group" ng-repeat="(budget,count) in skillslist.budgetMap">
						<input type="checkbox" checklist-model="budget1.budget" checklist-value="budget" ng-click="checkbox()"> {{budget}}({{count}}) &nbsp;
						
					</ul>
				</div>
				<br>
	
			</div>
		</div>	 		
					<div class="col-sm-8">
						<div ng-repeat="x in list | filter:search" class="box">
						
							
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="header-box">
								<p>
								<span title="Job Category">{{x.jobCategory}}</span><span title="Location">-{{x.currentLocation}}</span><span title="Experience">({{x.yearsOfExperience}}-{{x.monthsOfExperience}})</span>
								</p>
							</div></a>
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="row">
							<div class="companyName-box col-sm-7">
								<p><span title="Company Name">{{x.companyName}}</span> <span title="Resource Id">{{x.resId}}</span></p>
							</div>
							<div class="col-sm-5">
							<span style="" class="iconcolor" >&#x20B9;</span>&nbsp;:<span style="" class="textcolor" title="Budget">{{x.budget}} /</span><span style="" class="textfont">{{x.rate}}</span> +<span style="" class="taxes"> {{x.gst}} </span></span>
							</div>
							</div></a>
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="description-box">
								<p title="Resource Name"><i class="fa fa-user-circle-o" aria-hidden="true"></i>&nbsp;&nbsp;{{x.firstName}}</p>
							</div></a>
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="row">
							<div class="description-box col-sm-7">
								<p>
									<i class="glyphicon glyphicon-envelope" > </i>&nbsp;&nbsp;<span title="Vendor EmailId">{{x.emailId}}&nbsp;</span>
								</p>
							</div>
							<div class="col-sm-5">
							<i
										class="fa fa-phone iconcolor " ></i>&nbsp;:<span class="textcolor" title="Vendor Contact Number">{{x.mobileNumber}}</span>
							</div>
							</div></a>
							<a ui-sref="customer.resourcesummary({resourceId:x._id})"><div class="description-box">
								<p>
									<i class="glyphicon glyphicon-calendar"></i>&nbsp;&nbsp;{{x.availability}}
									 &nbsp;&nbsp;MSA Available&nbsp; : <a class="btn btn-sm btn-primary butn" ng-click="isSowUserResource(x)"  disabled>{{x.isSowUser}}</a>
								</p>
							</div></a>
							<div class="Skills-box">
								<p>
									Skills:{{x.primarySkills.toLowerCase()}}
									<button class="btn btn-primary pull-right" ng-click=requestResource(x._id,x.registrationId)>Request Resource</button>
								</p>
								
							</div>
						</div>	
					<button type="button" class="btn btn-danger" ui-sref="customer.postrequirement">
				<i class="	glyphicon glyphicon-share"></i>&nbsp;Go Back
			</button>		
			</div>
		</div>
	</div>

	
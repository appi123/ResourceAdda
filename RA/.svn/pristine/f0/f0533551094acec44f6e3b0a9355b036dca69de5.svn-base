<div class="container" ng-show="showMe">
<hr class="hr-bar">
<div class="row">

<h3 style="color: #000">Your Previous Plans </h3>


<div  class="container m-t-50">
               <div class="table-responsive m-t-30">
               	<table  class="table text-center">
   <thead>
       <th>S.No</th>
       <th>Rate</th>
       <th>Validity</th>
       <th>Vacancy</th>	       	      
<th>CV</th>
<th>Email</th>


   </thead>
   <tbody>
       <tr ng-repeat="plan in previousplanlist">
           <td>{{$index+1}}</td>
           <td>{{ plan.rate}}</td>
           <td>{{ plan.month}}</td>
           <td>{{plan.vacancy}}</td>
           <td>{{plan.cv}}</td>
           <td>{{plan.email}}</td>
           
           
          <!-- <td><button class="btn btn-primary"  ui-sref="RA.updateplan({planId: plan._id})"><i class="fa fa-pencil"></i>&nbsp;</button></td> -->
       </tr>
   </tbody>
</table>
               </div> 

</div>


<div class="container plan" style="margin-top: 10px;">

	<div class="panel col-md-6">
		<div class="panel-heading" 
			style="background-color: #777f7fd9 !important; padding: 1px 15px !important; color: white !important">
			<center>
				<h3>Your Subscription-Plan</h3>
			</center>
		</div>
		<div class="panel-body">
			<div class=" col-md-offset-3 col-md-">
				<table>
					<tr>
						<th>Period :</th>
						<td>{{planlist.period}}</td>
					</tr>
				
					<tr>
						<th>Rate :</th>
						<td>{{planlist.rate}}</td>
					</tr>
					<tr>
						<th>Email :</th>
						<td>{{planlist.email}}</td>
					</tr>
					<tr>
						<th>CV :</th>
						<td>{{planlist.cv}}</td>
					</tr>
					<tr>
						<th>Hot Vacancy :</th>
						<td>{{planlist.vacancy}}</td>
					</tr>
					<tr>
						<th>Remaining Days :</th>
						<td>{{noOfDays.noOfDays}}&nbspDays</td>
					</tr>
		
				</table>
				<br />
				<br />
				<div class="col-md-8 col-md-offset-1">
				<div class="row">
				<div class="col-sm-6">
				<a type="button" ui-sref="customer.buynew" class="btn btn-primary">Change
						Plan</a>
				</div>
				
				<div class="col-sm-6">
						<a class="btn btn-primary " ng-click="PreviousPlans()">Previous Plans</a>
				</div>
				</div>
					
				</div>
			</div>
		</div>
	</div>


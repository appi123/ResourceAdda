 <div  class="container m-t-50">
                <form class="form-inline text-center">
                    <div class="input-group">
                    <input type="text" ng-model="search" class="form-control" size="50" placeholder="Search requirement" required>
                    <div class="input-group-btn">
                    <button type="button" class="btn btn-primary">Search</button>
                    </div>
                    </div>
                </form> 
      <div class="table-responsive m-t-30">
           <table class="table text-center">
                      <thead>
                          <tr>
                              <th>S.No</th>

                                                <th>Job Type</th>
                                                <th>Job Description</th>
                                                <th>Job Location</th>
                                                <th>Experience</th>
                                                <th>Primary Skills</th>
                                                <th>No of Resources</th>
                                                <th>Join Within</th>
                                                <th>Status</th>
                                                <th>Edit</th>
                                                
                                          </tr>
                                        </thead>
                                        <tbody>
                                          <tr ng-repeat="x in list | filter:search">
                                            <td>{{$index+1}}</td>
                                            <td>{{x.jobTitle}}</td>
                                            <td>{{x.description}}</td>
                                            <td>{{x.jobLocation}}</td>
                                            <td>{{x.experience}}</td>
                                            <td>{{x.primarySkills}}</td>
                                            <td>{{x.resources}}</td>
                                            <td>{{x.joinWithin}}</td>
                                            <td><a type="button" class="btn btn-primary butn" ng-click="requirement(x)">{{x.status}}</a></td>
                                            <td><a type="button" class="btn btn-primary" ui-sref="RA.updatepostarequirement({postId:x._id})"><i class="fa fa-pencil" aria-hidden="true"></i></a></td>
                                           
                                          </tr>
                                        </tbody>
                                      </table>
                                  </div>
                                  <div class="row">
                                  <div class="col-md-6">
                                      <button type="button" class="btn btn-primary">Prev</button> 1/13 <button type="button" class="btn btn-primary">Next</button>
                                  </div>
                                  <div class="col-md-6">
                                      <button type="button" class="btn btn-primary pull-right" ui-sref="RA.addpostrequirement"> Add </button>
                                  </div>
                                  </div>
                              </div>